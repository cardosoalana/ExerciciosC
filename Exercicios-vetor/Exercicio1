// Leia um valor do teclado e imprima a correspondente quantidade de asteriscos. Ex. Se ler o numero 6 imprima ******
#include <stdio.h>

int main (void) {
	int valor = 0;
	
	printf("Insira um valor: \n");
	scanf("%d", &valor);
	
	char str[valor];

	
	for (int i = 0; i <= valor - 1; i++) {
		str[i] = '*';
	}
	
	for (int i = 0; i <= valor - 1; i++) {
		printf("%c", str[i]);
	}
}
